   "source": [
    "s = input(\"Enter the string\")\n",
    "co = 0\n",
    "cv = 0\n",
    "n = len(s)\n",
    "for i in range(0,n):\n",
    "    if(s[i] in 'aeiou' or s[i] in 'AEIOU'):\n",
    "        cv = cv+1\n",
    "    else:\n",
    "        co = co+1\n",
    "print(\"Number of vowels in\",s,\"are\",cv)\n",
    "print(\"Number of consonants in\",s,\"are\",co)"
